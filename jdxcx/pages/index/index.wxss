/**index.wxss**/
.load{
  top: 0;
}
.notice{
  top: 800rpx;
}
swiper{
  width: 100%;
  height: 340rpx;
  overflow: hidden;
}
swiper .slide-image{
  width: 100%;
  /* border-radius: 10rpx; */
  height: 100%;
}
.menu-list{
  width: 100%;
  display: flex;
  /* margin-top: 20rpx; */
  align-items: center;
  padding: 15rpx;
  /* background: #ff0000; */
  box-sizing: border-box;
  flex-wrap: wrap;
  margin-bottom: 10rpx;
  border-bottom: 10rpx solid #eee;
  border-top: 10rpx solid #eee;
}
.menu-list .menu-list-item{
  width: 25%;
  height: 180rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  /* background: #ddd; */
  margin-bottom: 5rpx;
}
.menu-list .menu-list-item image{
  width: 110rpx;
  height: 110rpx;
}
.menu-list .menu-list-item text{
  font-size: 30rpx;
  color: #515151;
}